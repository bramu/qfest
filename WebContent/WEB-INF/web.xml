<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>qfest</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>question</servlet-name>
    <servlet-class>com.tb.servlets.QuestionsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>question</servlet-name>
    <url-pattern>/questions</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>user</servlet-name>
    <servlet-class>com.tb.servlets.UsersServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>user</servlet-name>
    <url-pattern>/users</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ratings</servlet-name>
    <servlet-class>com.tb.servlets.RatingsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ratings</servlet-name>
    <url-pattern>/ratings</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>answers</servlet-name>
    <servlet-class>com.tb.servlets.AnswersServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>answers</servlet-name>
    <url-pattern>/answers</url-pattern>
  </servlet-mapping>
</web-app>